<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Attributproduit" table="attributproduit">
    <id name="idattributproduit" type="integer" column="idAttributProduit">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="integer" column="nom" nullable="false"/>
    <field name="categorieid" type="integer" column="categorieId" nullable="true"/>
    <many-to-many field="categoriecategory" target-entity="Categorie" mapped-by="attributproduitattributproduit"/>
  </entity>
</doctrine-mapping>
