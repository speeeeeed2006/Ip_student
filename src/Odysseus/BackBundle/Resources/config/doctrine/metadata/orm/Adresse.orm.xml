<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Adresse" table="adresse">
    <indexes>
      <index name="fk_Adresse_Etat1_idx" columns="etatId"/>
    </indexes>
    <id name="idadresse" type="integer" column="idAdresse">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="boolean" column="type" nullable="false"/>
    <field name="adresse" type="string" column="adresse" length="255" nullable="false"/>
    <field name="cp" type="integer" column="cp" nullable="false"/>
    <field name="ville" type="string" column="ville" length="60" nullable="false"/>
    <field name="pays" type="string" column="pays" length="45" nullable="false"/>
    <field name="idclient" type="integer" column="idClient" nullable="false"/>
    <field name="montant" type="integer" column="montant" nullable="false"/>
    <field name="modepaiement" type="boolean" column="modePaiement" nullable="false"/>
    <many-to-one field="etatid" target-entity="Etat">
      <join-columns>
        <join-column name="etatId" referenced-column-name="idEtat"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
