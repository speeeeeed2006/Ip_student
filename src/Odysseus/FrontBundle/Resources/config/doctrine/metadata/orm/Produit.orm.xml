<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Produit" table="produit">
    <indexes>
      <index name="fk_Produit_SousCategorie1_idx" columns="sousCategorieId"/>
      <index name="fk_Produit_Etat1_idx" columns="etatId"/>
    </indexes>
    <id name="idproduit" type="integer" column="idProduit">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="reference" type="string" column="reference" length="40" nullable="false"/>
    <field name="nom" type="integer" column="nom" nullable="false"/>
    <field name="description" type="string" column="description" length="45" nullable="false"/>
    <field name="stock" type="integer" column="stock" nullable="false"/>
    <field name="categorieid" type="integer" column="categorieId" nullable="false"/>
    <field name="produitcol" type="string" column="Produitcol" length="45" nullable="false"/>
    <field name="image" type="string" column="image" length="45" nullable="false"/>
    <many-to-one field="souscategorieid" target-entity="Souscategorie">
      <join-columns>
        <join-column name="sousCategorieId" referenced-column-name="idSousCategorie"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="etatid" target-entity="Etat">
      <join-columns>
        <join-column name="etatId" referenced-column-name="idEtat"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="commandecommande" target-entity="Commande" mapped-by="produitproduit"/>
  </entity>
</doctrine-mapping>
